{
  "name": "openapi-golang-typescript-frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint 'src/**/*{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*{ts,tsx}'",
    "format": "prettier --check 'src/**/*{ts,tsx}'",
    "format:fix": "prettier --write 'src/**/*{ts,tsx}'",
    "format:gen": "prettier --write 'src/adapters/gen/*{ts,tsx}'",
    "codegen:oapi": "rimraf ./src/adapters/gen && openapi-generator generate -g typescript-axios -i ../schema/openapi.yaml -o ./src/adapters/gen"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "http-server": "^14.1.1",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
